<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.all.mapper.loginmapper"> 
    <insert id="join" parameterType="com.example.all.Tb_Member">
	    insert into tb_member(m_id,m_pw,m_age,m_gender,m_disease,m_name,m_cheatingday,m_weight)
	    values(#{m_id},#{m_pw},28,#{m_gender},#{m_disease},#{m_name},#{m_cheatingday},#{m_weight})
    </insert>
</mapper>